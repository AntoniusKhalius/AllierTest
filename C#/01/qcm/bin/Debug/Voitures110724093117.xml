<?xml version="1.0" encoding="iso-8859-1" standalone="yes"?>
<questionnaire name='Voitures' displayName='Questionnaire sur votre voiture'>
<description>Des questions
sur la marque, le carburant et la 
couleur</description>
	<question type='combo' name='Marque'>
		<text>La marque de votre voiture :</text>
		<reponses>
			<reponse default ='false'>Mercedes</reponse>
			<reponse default ='true'>Volkswagen</reponse>
			<reponse default ='false'>Renault</reponse>
			<reponse default ='false'>Peugeot</reponse>
			<reponse default ='false'>Citroen</reponse>
		</reponses>
	</question>
	<question type='radio' name='Carburant'>
		<text>Choisir le carburant</text>
		<reponses>
			<reponse default ='true'>Essence</reponse>
			<reponse default ='false'>Diesel</reponse>
			<reponse default ='false'>GPL</reponse>
			<reponse default ='false'>Electricite</reponse>
		</reponses>
	</question>
	<question type='text' name='Couleur'>
		<text>Saisir la couleur :</text>
		<defaultreponse>Claire</defaultreponse>
		<maxCharacters>75</maxCharacters>
	</question>
</questionnaire>
