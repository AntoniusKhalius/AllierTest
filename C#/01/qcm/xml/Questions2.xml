<?xml version="1.0"?> 
<questionnaire name="AppliQuestions" displayName="Questionnaire">
	<description>Questionnaire sur la France</description>
	<question type="combo" name="Question sur les villes">
		<text>Quelle est la capitale de la France ?</text>
		<reponses>
			<reponse default = 'false'>Lyon</reponse>
			<reponse default = 'false'>Bordeaux</reponse>
			<reponse default = 'false'>Paris</reponse>
			<reponse default = 'false'>Marseille</reponse>
		</reponses>	
	</question>
	<question type="text" name="Question sur votre ville">
		<text>Quel ville habitez-vous ?</text>
		<defaultreponse>Vichy</defaultreponse>
		<maxCharacters>30</maxCharacters>
	</question>
	<question type="radio" name="Question sur le sexe">
		<text>Quel est votre sexe ?</text>
		<reponses>
			<reponse default="true">Masculin</reponse>
			<reponse>Feminin</reponse>
		</reponses>	
	</question>
</questionnaire>
